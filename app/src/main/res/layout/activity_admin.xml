<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="comvoice.example.zhangbin.startimage.activity.AdminActivity"
    android:orientation="vertical">
    <include layout="@layout/title_bar"/>

    <Button
        android:id="@+id/bt_delete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/buttonWidth"
        android:layout_marginLeft="@dimen/buttonWidth"
        android:text="@string/delete"
        android:background="@drawable/btn_bg"
        android:layout_marginTop="@dimen/buttonWidth"/>
</LinearLayout>
